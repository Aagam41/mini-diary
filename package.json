{
	"name": "mini-diary",
	"version": "0.1.0",
	"main": "main.js",
	"private": true,
	"scripts": {
		"start": "run-p start:*",
		"start:webpack": "webpack --mode development --watch",
		"start:electron": "electron .",
		"build": "run-s build:*",
		"build:webpack": "webpack --mode production",
		"build:electron": "electron-packager . --out=dist --all --asar --overwrite",
		"lint": "eslint src"
	},
	"dependencies": {
		"electron-debug": "^2.0.0",
		"electron-settings": "^3.2.0",
		"minireset.css": "^0.0.3",
		"prop-types": "^15.6.2",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-redux": "^5.0.7",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"@babel/preset-react": "^7.0.0",
		"babel-loader": "^8.0.4",
		"css-loader": "^1.0.0",
		"electron": "^3.0.2",
		"electron-packager": "^12.1.0",
		"eslint": "^5.6.1",
		"eslint-config-airbnb": "^17.0.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.10.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^1.0.1",
		"node-sass": "^4.9.2",
		"npm-run-all": "^4.1.3",
		"sass-loader": "^7.0.3",
		"style-loader": "^0.23.0",
		"url-loader": "^1.1.1",
		"webpack": "^4.16.1",
		"webpack-cli": "^3.0.8"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint",
			"pre-push": "yarn lint"
		}
	}
}
